function getWidgetEmbedCode(){

        var url = $('#fundUrl').val(),
            template_id = $('#hidden_templateid').val();

        return '<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="258" height="338" title="Click Here to donate!" type="application/x-shockwave-flash">'
            + '<param name="movie" value="//funds.gofundme.com/Widgetflex.swf" />'
            + '<param name="quality" value="high" />'
            + '<param name="flashvars" value="page='+url+'&template='+template_id+'" />'
            + '<param name="wmode" value="transparent" />'
            + '<embed allowScriptAccess="always" src="//funds.gofundme.com/Widgetflex.swf" quality="high" flashVars="page='+url+'&template='+template_id+'" type="application/x-shockwave-flash" wmode="transparent" width="258" height="338"></embed>'
            + '</object>';

}


function getWidgetSourceUrl(){
        var url = $('#fundUrl').val().replace('/','');

        return url2="/web/20141030082225/http://funds.gofundme.com/index.php?route=widgets/typeb"+
            "&url="+url+"&c=0&t="+$('#hidden_templateid').val();
}

function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

$(".widgetLb").click(function(){
        $("#widgetCode").val(getWidgetEmbedCode());
        $("#widgetPreviewIframe").attr("src",getWidgetSourceUrl() + "&iframe=1");

        $("#widget_lb").showAsLightbox(true);
        return false;
});


$(function(){
        if(getParameterByName('notready') == 1){
                showNotReadyLB();
        }else if(getParameterByName('stop') == 1){
                showDonationsStoppedLB();
        }
});

