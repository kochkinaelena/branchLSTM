$('.followContinue').click(function() {
        $(this).hide();
        $(".subscribeUpdates").html("&nbsp;");
        $('.followLoading').show();
        GFM.Follower.fbconnect('id_share_btn');
        return false;
});

function showDonationsStoppedLB(){
        $('#campaignFinishedLightbox').showAsLightbox(true);
}

function showFbFriendsLb(x){
        if (x.friend_count==1)
                $('#friendsFoundLabel').html(x.friend_count+" Friend Found!");
        else
                $('#friendsFoundLabel').html(x.friend_count+" Friends Found!");

        var buf = "",
            flen = x.friends.length;

        for (var i=0; i<flen; i++) {
                if(i>4) break;
                var curr = x.friends[i];

                buf += "<div class='dpic2 mr20'>";
                buf += '<a href="'+x.fbfurl+'" target="_blank"><img src="'+curr.picurl+'" title="'+curr.fbname+'"/></a>';
                buf += "</div>";

        }
        $('#friend_pic_container').html(buf);
        $('#friend_pic_container .dpic2').last().removeClass("mr20");
        $('#id_lnk_fbf').attr('href', x.fbfurl);
        $('#follow_lb').hide();
        $('#follow_thankyou_lbfriends').showAsLightbox();
}

$('.share-tw').click(function () {
        window_open('/web/20141030082225/https://twitter.com/intent/tweet?text='+ $('#hidden_fundname').val() +
            '&url='+encodeURIComponent(decodeURIComponent($('#hidden_fund_min_url').val())+"?pc="+$(this).attr('data-pc'))+
            '&via=gofundme'
        );
        return false;
});

function window_open(url)
{
        var width= 626;
        var height=436;

        var leftPosition, topPosition;
        leftPosition = (window.screen.width / 2) - ((width / 2) + 10);
        topPosition = (window.screen.height / 2) - ((height / 2) + 50);

        var windowFeatures = "status=no,height=" + height + ",width=" + width + ",resizable=yes,left=" + leftPosition + ",top=" + topPosition + ",screenX=" + leftPosition + ",screenY=" + topPosition + ",toolbar=no,menubar=no,scrollbars=no,location=no,directories=no";
        var social_w = window.open(url,'_blank', windowFeatures);

        if(social_w)
                social_w.focus();

        return false;
}

var $a= $(".a");

$a.on("click",".donationsPage",function(){
        var type = ($(this).data("type") !== undefined ? $(this).data("type") : $("#donationSort").val()),
            index = $(this).data("index");

        $.ajax({url: '//www.gofundme.com/mvc.php?route=donate/pagingdonationsb&url=werallone&idx='+index+"&type="+type,
                async: true,
                type:"GET",
                success: function(data)
                {
                        $('.donate_right').replaceWith(data);
                        $('.donate_right').scrollTo();
                }
        });

        return false;
});

$a.on("click",".donationToggle",function(){
        $(".toggleoff,.toggle").toggle();
        return false;
});

$a.on("click",".commentPaging",function(){
        var index = $(this).data("index");

        $.ajax({url: '//www.gofundme.com/mvc.php?route=donate/pagingcomments14&url=werallone&idx='+index,
                async: true,
                type:"GET",
                success: function(data)
                {
                        $('#commentBox').replaceWith(data);
                        $('#commentBox').scrollTo();
                        checkComment();
                }
        });

        return false;
});

$a.on("click",".doFbComment",function(){
        GFM.facebook.fbAssoc(false,true,function(){
                //change classes on the different states and slide down up instead of show hide
                $("#leaveCommentBox").slideUp();
                $("#commentInputBox").slideDown();
        });
        return false;
});

$a.on("click",".commentFbLogout",function(){
        GFM.facebook.logOut(function(){
                $("#commentInputBox").slideUp();
                $("#leaveCommentBox").slideDown();
        });
        return false;
});

$a.on("click",".submitComment",function(){
        fns.handlers.submit($("#commentForm"));
        return false;
});

//callback on successfull comment
function makecomment(cid)
{
        $.ajax({url: '//www.gofundme.com/mvc.php?route=donate/singlecomment&cid='+cid,
                async: true,
                type:"GET",
                success: function(data)
                {
                        var $new = $(data);

                        $new.hide();
                        var numPrevComments = $(".singlecomment").length;
                        if( numPrevComments > 0 ){
                            $(".singleComment").first().before($new);
                            $('#commentBox .section_head').html( "" + (numPrevComments+1) + " COMMENTS" );
                        }else{//this is the first comment
                            $("#commentInputBox").after($new);
                            $('#commentBox .section_head').html("1 COMMENT");
                        }

                        $("#commentInputBox").fadeOut(function(){
                            $new.slideDown();
                            $("#commentPostedMessage").slideDown();
                        });
                        $("#commentTextBox").val("");

                        setTimeout(function(){
                                $(".fadeOutMessage:visible").slideUp(function(){
                                    $("#commentInputBox").slideDown();
                                });

                        },3000);

                        $(".comname_det span").each(function(){
                                if(!$(this).hasClass("fb_first_name")) $(this).hide()
                        });
                        checkComment();
                }
        });
}

function checkComment(){
        if($.cookie("enc_c") !== undefined){
                $.ajax({
                        url: "//www.gofundme.com/mvc.php?route=donate/commentcheck&url=werallone&encc="+ $.cookie("enc_c"),
                        async: true,
                        success: function(data){
                                if(data != ""){
                                        $("#commentId_"+data).show();
                                }
                        }
                });
        }
}

setTimeout(checkComment,100);

$a.on("click",".deleteCommentToggle",function(){
        var cId = $(this).data("id");
        $("#commentId_"+cId).toggle();
        $("#cConfirmId_"+cId).toggle();
        return false;
});

$a.on("click",".confirmDeleteComment",function(){
        var cId = $(this).data('id');
        var $that = $(this);
        $.ajax({
                url: "//www.gofundme.com/mvc.php?route=donate/commentdelete&url=werallone&cid="+cId,
                async: true,
                success: function(data){
                        $that.closest(".dcomment").slideUp();

                        $("#commentInputBox,#leaveCommentBox").slideUp();
                        $("#commentRemovedMessage").slideDown();

                        setTimeout(function(){
                                $(".fadeOutMessage").slideUp();
                                $("#leaveCommentBox").slideDown();
                        },3000);
                }
        });
        return false;
});

