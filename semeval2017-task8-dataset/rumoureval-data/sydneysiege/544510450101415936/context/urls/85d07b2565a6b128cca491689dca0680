        $(function(){
                $(".searchToggle").click(function(){
                        $('.preSearch').hide();
                        $('#searchMenu').animate({width:'toggle'});
                        return false;
                });

                $(".categoryToggle").click(function(){
                        $(".cat_drop").slideToggle();
                        return false;
                });

                $("body").click(function(){
                        $(".cat_drop").slideUp();
                });
        });
</script>


