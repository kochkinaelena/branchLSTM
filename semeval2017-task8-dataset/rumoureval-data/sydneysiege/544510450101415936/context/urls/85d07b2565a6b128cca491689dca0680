<script>
$('.donateButton').click(function () {
                var url = "/web/20141030082225/https://www.gofundme.com/WeRaLLONe/donate";
        if ($(this).data("wl") !== undefined) {
                url += "&wl=" + $(this).data("wl");
        } else if ($(this).data("amt") !== undefined) {
                url += "&amt=" + $(this).data("amt");
        }
        try {
                window.top.location.href = url;
        } catch (e) {
                location.href = url;
        }
        
        return false;
});

$('.share-fb').click(function() {
        $(".followContinue").show();
        $('.followLoading').hide();
        try              { GFM.Follower.post2feed.apply($(this)); }
        catch (notready) { setTimeout(function() {GFM.Follower.post2feed.apply($(this))}, 1000); }
        return false;
});

$(".closeLightbox").click(function(){
        $(".gfm_lb:visible").closeLightbox();
        return false;
});

